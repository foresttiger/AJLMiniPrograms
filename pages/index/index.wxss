.log-item{
  font-size: 24px;
  letter-spacing: 5px;
  padding-bottom: 2px;
  font-weight: 600;
  border-bottom:2px solid #454545;
  margin-bottom: 20px;
  text-align: center;
  margin-top: -50px;
}
.fit{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  width: 70%;
  height: 70px;
  position: relative;
}
.fit .line{
  width: 100%;
  position: absolute;
  height: 1px;
  background-color: #5f5f5f;
  top: 50%;
  left: 0;
  z-index: 100;
}
.fit-line{
  color:#5f5f5f;
}
.fit-head{
  font-size: 20px;
}
.v-line{
  font-size: 16px;
  width: 160px;
  text-align: center;
  margin:0;
  position: relative;
  background-color: #ffd401;
  /* margin: 3px 0; */
  z-index: 101;
  height: 30px;
  line-height: 30px;
}
.btn{
  color:#ffffff;
  font-size: 20px;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  margin: 30px 0 0 0;
  width: 130px;
  height: 130px;
  background-color: #ffffff;
  border-radius: 130px;
  /* line-height: 140px; */
  text-align: center;
    /* background: #ff6d15; */
    cursor: pointer;
    transition: all .2s ease-in-out;
    border:2px solid #ffffff;
    background: #fdcc9a;
    z-index: 12;
    -webkit-animation: scale-three2 1.5s ease infinite;
    -moz-animation: scale-three2 1.5s ease infinite;
    -o-animation: scale-three2 1.5s ease infinite;
    animation: scale-three2 1.5s ease infinite;
    animation-fill-mode: both;
}
.btn .btn-text{
  /* font-size: 23px;
  margin:0px; */
  width: 48px;
  height: 48px;
  font-size: 23px;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.btn text{
  display: flex;
  align-content: center;
  width: 24px;
  height: 24px;
  line-height: 24px;
}
.tips{
  position: absolute;
  bottom:6px;
  font-size: 12px;
}